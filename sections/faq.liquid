{%- if section.blocks.size > 0 -%}
  <div class="color-scheme color-scheme--{{ section.settings.color_scheme.id }}">
    {%- capture section_header -%}
      {%- if section.settings.subheading != blank or section.settings.title != blank or section.settings.content != blank or section.settings.button_text != blank -%}
        <div class="section-header">
          <div class="prose {% if section.settings.layout == 'stacked' %}prose-justify-center{% endif %}">
            {%- render 'subheading', content: section.settings.subheading -%}
            {%- render 'heading', content: section.settings.title, balance_text: true, style: section.settings.title_tag, html_tag: 'h2' -%}

            <div class="max-w-sm">
              {{- section.settings.content -}}
            </div>

            {%- render 'button', content: section.settings.button_text, href: section.settings.button_url, style: section.settings.button_style -%}
          </div>
        </div>
      {%- endif -%}
    {%- endcapture -%}

    {%- capture section_content -%}
      <div class="faq">
        <div class="faq__content">
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when 'category' -%}
                {%- if block.settings.title != blank -%}
                  <div class="faq__category">
                    {%- if block.settings.custom_icon != blank -%}
                      {%- render 'icon', image: block.settings.custom_icon, width: block.settings.icon_width, mobile_width: block.settings.mobile_icon_width -%}
                    {%- elsif block.settings.icon != 'none' -%}
                      {%- render 'icon' with block.settings.icon, width: block.settings.icon_width, mobile_width: block.settings.mobile_icon_width -%}
                    {%- endif -%}

                    <p class="h6 text-start" {{ block.shopify_attributes }}>{{ block.settings.title }}</p>
                  </div>
                {%- endif -%}
  
              {%- when 'question' -%}
                {%- liquid
                  assign use_secondary_background = false

                  if section.settings.accordion_style == 'background' and section.settings.color_scheme.settings.background_gradient != blank or section.settings.color_scheme.settings.background != section.settings.color_scheme.settings.secondary_background
                    assign use_secondary_background = true
                  endif

                  if block.settings.question != blank and block.settings.answer != blank
                    if section.settings.question_style == 'subheading'
                      render 'accordion', title: block.settings.question, content: block.settings.answer, prose_content: true, subheading_title: true, icon: block.settings.icon, custom_icon: block.settings.custom_icon, icon_width: block.settings.icon_width, mobile_icon_width: block.settings.mobile_icon_width, use_secondary_background: use_secondary_background, block: block
                    else
                      render 'accordion', title: block.settings.question, content: block.settings.answer, prose_content: true, icon: block.settings.icon, custom_icon: block.settings.custom_icon, icon_width: block.settings.icon_width, mobile_icon_width: block.settings.mobile_icon_width, use_secondary_background: use_secondary_background, block: block
                    endif
                  endif
                -%}
            {%- endcase -%}
          {%- endfor -%}
        </div>
      </div>
    {%- endcapture -%}

    {%- if section.settings.layout == 'split' -%}
      <div class="section-split">
        <div class="section-split__column">
          {{- section_header -}}
        </div>

        <div class="section-split__column">
          {{- section_content -}}
        </div>
      </div>
    {%- else -%}
      <div class="container section-spacing">
        <div class="section-stack max-w-lg mx-auto text-center">
          {{- section_header -}}
          {{- section_content -}}
        </div>
      </div>
    {%- endif -%}
  </div>

  {%- comment -%}We also output the content with JSON microdata for SEO{%- endcomment -%}

  <script type="application/ld+json">
    {%- assign question_blocks = section.blocks | where: 'type', 'question' -%}

    {
      "@context": "https://schema.org",
      "@type": "FAQPage",
      "mainEntity": [
        {%- for block in question_blocks -%}
          {
            "@type": "Question",
            "name": {{ block.settings.question | json }},
            "acceptedAnswer": {
              "@type": "Answer",
              "text": {{ block.settings.answer | json }}
            }
          }{% unless forloop.last %},{% endunless %}
        {%- endfor -%}
      ]
    }
  </script>
{%- endif -%}

